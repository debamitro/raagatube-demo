html
  head
    title RaagaTube
    link(href='../css/basscss.css' rel='stylesheet')
    link(href='../css/btns.css' rel='stylesheet')
  body
    div(class='clearfix')
      div(class='col col-4')
        a(href='../..' class='h1') RaagaTube
      div(class='col col-8')
        a(href='/' class='btn btn--s') Browse by Raaga
        a(href='..' class='btn btn--s') Browse by Instrument
        a(href='..' class='btn btn--s') Browse by Artist
    div(class='clearfix')
      div(class='col col-2')
        ul(class='list-reset')
          - raagaList.forEach ( (item) => {
          li
            != item
          - })
      div(class='col col-10')
        div(class='clearfix h2 italic')
          = raagaName
        div(class='clearfix')
          ul(class='list-reset')
            - videos.forEach ( (item) => {
            li
              != item
            - })
